{
  "private": true,
  "name": "my-website",
  "description": "My personal website.",
  "version": "3.0.0",
  "author": "Jacob Alspaw <dev@jacob-alspaw.com>",
  "packageManager": "npm@10.9.0",
  "engines": {
    "node": ">=22.12.0 < 23"
  },
  "type": "module",
  "scripts": {
    "prepare": "husky",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint:ts": "eslint . --ext .js,.mjs,.ts,.vue --quiet",
    "lint:ts:fix": "eslint . --ext .js,.mjs,.ts,.vue --quiet --fix",
    "lint:style": "stylelint \"**/*.{css,vue}\"",
    "lint:style:fix": "stylelint \"**/*.{css,vue}\" --fix",
    "lint": "npm run lint:ts && npm run lint:style",
    "lint:fix": "npm run lint:ts:fix && npm run lint:style:fix",
    "format": "prettier \"**/*.{js,mjs,ts,vue,css}\"",
    "format:fix": "prettier \"**/*.{js,mjs,ts,vue,css}\" --write"
  },
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "floating-vue": "^5.2.2",
    "globe.gl": "^2.45.0",
    "satellite.js": "^6.0.2",
    "three": "^0.182.0",
    "topojson-client": "^3.1.0",
    "vue": "^3.5.27",
    "vue-photo-album": "^1.2.4",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.3.1",
    "@commitlint/config-conventional": "^20.3.1",
    "@types/three": "^0.182.0",
    "@types/topojson-client": "^3.1.5",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@vitejs/plugin-vue": "^6.0.3",
    "commitlint": "^20.3.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.3.0",
    "eslint-plugin-vue": "^10.7.0",
    "globals": "^17.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "postcss-html": "^1.8.1",
    "prettier": "^3.8.1",
    "stylelint": "^17.0.0",
    "stylelint-config-recommended": "^18.0.0",
    "stylelint-config-recommended-vue": "^1.6.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "vite": "^7.3.1",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.2.3"
  }
}
